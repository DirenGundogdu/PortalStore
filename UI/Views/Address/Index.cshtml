@model Resultmodel<DTO.AddressDto>


    <div class="container">
        <a type="button" class="btn my-3 btn-outline-success" asp-controller="Address" asp-action="Create" asp-route-customerId="@ViewBag.customerId">Create</a>
        <table class="table table-dark table-striped">
            <thead>
                <tr>
                    <th scope="col">Addressline</th>
                    <th scope="col">Country</th>
                    <th scope="col">City</th>
                    <th scope="col">Distrinct</th>
                    <th scope="col">ZipCode</th>
                    <th scope="col">Update</th>
                    <th scope="col">Delete</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.DataList != null)
                {
                    foreach (var item in Model.DataList)
                    {
                        <tr>
                            <th scope="row">@item.AddressLine</th>
                            <td>@item.Country</td>
                            <td>@item.City</td>
                            <td>@item.District</td>
                            <td>@item.ZipCode</td>
                            <td><a type="button" class="btn btn-outline-warning" asp-controller="Address" asp-action="Update" asp-route-id="@item.Id">Update</a></td>
                            <td><a type="button" class="btn-close btn-close-white" asp-controller="Address" asp-action="Remove" asp-route-id="@item.Id"></a></td>
                        </tr>
                    }
                }

            </tbody>
        </table>
    </div>